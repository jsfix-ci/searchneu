{
  "name": "searchneu-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:db:up": "docker-compose -p searchneu_users -f infrastructure/dev/docker-compose.yml up -d",
    "dev:db:down": "docker-compose -p searchneu_users -f infrastructure/dev/docker-compose.yml down",
    "dev": "concurrently -n app,codegen \"next dev -p 5000\" \"npm:generate:graphql -- --watch\"",
    "build": "next build",
    "start": "next start -p 5000",
    "test:frontend": "jest",
    "style": "yarn prettier --write . --ignore-path .gitignore",
    "lint": "yarn eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "test:api": "jest --projects tests/pages/api/utils --testPathPattern",
    "generate:graphql": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@prisma/client": "2.13.0",
    "@types/axios": "^0.14.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pg": "^7.14.7",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "amplitude-js": "^7.3.3",
    "async-retry": "^1.3.1",
    "axios": "^0.21.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cookie": "^0.4.1",
    "crypto": "^1.0.1",
    "dayjs": "^1.9.8",
    "enzyme": "^3.11.0",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "interweave": "^12.6.0",
    "is-mobile": "^2.2.2",
    "jest-svg-transformer": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mock-local-storage": "^1.1.15",
    "next": "10.0.4",
    "pg": "^8.5.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-infinite-scroll-component": "^5.1.0",
    "react-tooltip": "^4.2.11",
    "react-transition-group": "^4.4.1",
    "reflect-metadata": "^0.1.13",
    "rollbar": "^2.19.4",
    "sass": "^1.30.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "swr": "^0.3.11",
    "use-deep-compare-effect": "^1.6.1",
    "use-query-params": "^1.1.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-graphql-request": "^3.0.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@next/bundle-analyzer": "^10.0.4",
    "@prisma/cli": "^2.13.0",
    "@svgr/webpack": "^5.5.0",
    "@types/amplitude-js": "^7.0.0",
    "@types/cookie": "^0.4.0",
    "@types/gtag.js": "^0.0.4",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.166",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-jest": "^26.6.3",
    "concurrently": "^5.3.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "next-compose-plugins": "^2.2.1",
    "next-test-api-route-handler": "^1.1.3",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
